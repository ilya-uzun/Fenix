		ELSIF  NumberMessage.12     THEN tmp2:='  ';
		ELSIF  NumberMessage.13     THEN tmp2:='  ';
		ELSE  tmp2:='Норма';
		END_IF


		CASE ControlState OF
		0:
				tmp1:='Блокировка';
			1:
				tmp1:='Аварийн. останов';
			2:
				IF NOT t11.Q THEN tmp1 := 'Вынужд. останов';
					ELSE tmp1:='ВО окончен';
				END_IF;
			3:
				tmp1:='СТОП';
